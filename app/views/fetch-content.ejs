<%- include('./_partials/_head') %>

<% if(code){ %>
    <form action="/fetch-content" method="post">
        <input type="hidden" value="<%= code %>" name="authorizedToken"/>
        <input type="submit" value="Fetch and edit my images" name="submit">
    </form>
<% } else{ %>
    <p>Unfortunatelly, no token has been received from the server <br> try again</p>
<% } %>

